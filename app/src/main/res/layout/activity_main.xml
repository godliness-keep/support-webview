<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <Button
        android:id="@+id/webview_native"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="原生 WebView" />


    <Button
        android:id="@+id/webview_native_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="原生 WebView Fragment" />

    <Button
        android:id="@+id/webview_x5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="X5 WebView" />

    <Button
        android:id="@+id/webview_x5_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="X5 WebView Fragment" />

</LinearLayout>